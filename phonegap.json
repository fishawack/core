{
    "widget": {
        "@xmlns": "http://www.w3.org/ns/widgets",
        "@xmlns:gap": "http://phonegap.com/ns/1.0",
        "@id": "<%= contentJson.attributes.phonegap.bundle %>",
        "@version": "<%= pkg.version %>",
        "name": "<%= contentJson.attributes.title %>",
        "author": {
            "@href": "http://www.fishawack.com",
            "@email": "digital@f-grp.com",
            "#text": "Fishawack"
        },
        "plugin": [
            {
                "@name": "cordova-plugin-device",
                "@spec": "2.0.2",
                "@source": "npm"
            },
            {
                "@name": "cordova-plugin-inappbrowser",
                "@spec": "3.0.0",
                "@source": "npm"
            },
            {
                "@name": "cordova-plugin-whitelist",
                "@spec": "1.3.3",
                "@source": "npm"
            },
            {
                "@name": "cordova-plugin-statusbar",
                "@spec": "2.4.2",
                "@source": "npm"
            },
            {
                "@name": "cordova-plugin-file",
                "@spec": "6.0.1",
                "@source": "npm"
            },
            {
                "@name": "cordova-plugin-email",
                "@spec": "1.2.7",
                "@source": "npm"
            }
        ],
        "preference": [
            {
                "@name": "phonegap-version",
                "@value": "<%= contentJson.attributes.phonegap.phonegapVersion %>"
            },
            {
                "@name": "AppendUserAgent",
                "@value": "Phonegap"
            },
            {
                "@name": "AllowInlineMediaPlayback",
                "@value": "true"
            },
            {
                "@name": "StatusBarStyle",
                "@value": "default"
            },
            {
                "@name": "StatusBarOverlaysWebView",
                "@value": "true"
            }
        ],
        "allow-navigation": {
            "@href": "*"
        },
        "allow-intent": {
            "@href": "*"
        },
        "access": {
            "@origin": "*"
        },
        "platform": [
            {
                "@name": "ios",
                "icon": [
                    {
                        "@src": "media/content/icons/ios/icon.png",
                        "@width": "57",
                        "@height": "57"
                    },
                    {
                        "@src": "media/content/icons/ios/icon@2x.png",
                        "@width": "114",
                        "@height": "114"
                    },
                    {
                        "@src": "media/content/icons/ios/icon-small-40.png",
                        "@width": "40",
                        "@height": "40"
                    },
                    {
                        "@src": "media/content/icons/ios/icon-small-40@2x.png",
                        "@width": "80",
                        "@height": "80"
                    },
                    {
                        "@src": "media/content/icons/ios/icon-small-40@3x.png",
                        "@width": "120",
                        "@height": "120"
                    },
                    {
                        "@src": "media/content/icons/ios/icon-small-50.png",
                        "@width": "50",
                        "@height": "50"
                    },
                    {
                        "@src": "media/content/icons/ios/icon-small-50@2x.png",
                        "@width": "100",
                        "@height": "100"
                    },
                    {
                        "@src": "media/content/icons/ios/icon-60.png",
                        "@width": "60",
                        "@height": "60"
                    },
                    {
                        "@src": "media/content/icons/ios/icon-60@2x.png",
                        "@width": "120",
                        "@height": "120"
                    },
                    {
                        "@src": "media/content/icons/ios/icon-60@3x.png",
                        "@width": "180",
                        "@height": "180"
                    },
                    {
                        "@src": "media/content/icons/ios/icon-72.png",
                        "@width": "72",
                        "@height": "72"
                    },
                    {
                        "@src": "media/content/icons/ios/icon-72@2x.png",
                        "@width": "144",
                        "@height": "144"
                    },
                    {
                        "@src": "media/content/icons/ios/icon-76.png",
                        "@width": "76",
                        "@height": "76"
                    },
                    {
                        "@src": "media/content/icons/ios/icon-76@2x.png",
                        "@width": "152",
                        "@height": "152"
                    },
                    {
                        "@src": "media/content/icons/ios/icon-small.png",
                        "@width": "29",
                        "@height": "29"
                    },
                    {
                        "@src": "media/content/icons/ios/icon-small@2x.png",
                        "@width": "58",
                        "@height": "58"
                    },
                    {
                        "@src": "media/content/icons/ios/icon-small@3x.png",
                        "@width": "87",
                        "@height": "87"
                    },
                    {
                        "@src": "media/content/icons/ios/icon-83.5@2x.png",
                        "@width": "167",
                        "@height": "167"
                    },
                    {
                        "@src": "media/content/icons/ios/icon-167.png",
                        "@width": "167",
                        "@height": "167"
                    },
                    {
                        "@src": "media/content/icons/ios/icon-1024.png",
                        "@width": "1024",
                        "@height": "1024"
                    }
                ],
                "splash": [
                    {
                        "@src": "media/content/screen/ios/Default@2x~iphone~anyany.png"
                    },
                    {
                        "@src": "media/content/screen/ios/Default@2x~iphone~comany.png"
                    },
                    {
                        "@src": "media/content/screen/ios/Default@3x~iphone~anyany.png"
                    },
                    {
                        "@src": "media/content/screen/ios/Default@3x~iphone~comany.png"
                    },
                    {
                        "@src": "media/content/screen/ios/Default@2x~ipad~anyany.png"
                    },
                    {
                        "@src": "media/content/screen/ios/Default@2x~ipad~comany.png"
                    }
                ]
            }
        ]
    }
}