{
    "widget": {
        "@xmlns": "http://www.w3.org/ns/widgets",
        "@xmlns:gap": "http://phonegap.com/ns/1.0",
        "@id": "<%= contentJson.attributes.phonegap.bundle %>",
        "name": "<%= contentJson.attributes.title %>",
        "author": {
            "@href": "http://www.fishawack.com",
            "@email": "digital@f-grp.com",
            "#text": "Fishawack"
        },
        "plugin": [
            {
                "@name": "cordova-plugin-device",
                "@spec": "2.0.2",
                "@source": "npm"
            },
            {
                "@name": "cordova-plugin-inappbrowser",
                "@spec": "3.0.0",
                "@source": "npm"
            },
            {
                "@name": "cordova-plugin-whitelist",
                "@spec": "1.3.3",
                "@source": "npm"
            },
            {
                "@name": "cordova-plugin-statusbar",
                "@spec": "2.4.2",
                "@source": "npm"
            },
            {
                "@name": "cordova-plugin-file",
                "@spec": "6.0.1",
                "@source": "npm"
            },
            {
                "@name": "cordova-plugin-email",
                "@spec": "1.2.7",
                "@source": "npm"
            }
        ],
        "preference": [
            {
                "@name": "phonegap-version",
                "@value": "<%= contentJson.attributes.phonegap.phonegapVersion %>"
            },
            {
                "@name": "StatusBarOverlaysWebView",
                "@value": "false"
            }
        ],
        "allow-navigation": {
            "@href": "*"
        },
        "allow-intent": {
            "@href": "*"
        },
        "access": {
            "@origin": "*"
        },
        "platform": [
            {
                "@name": "ios"
            }
        ],
        "icon": [
            {
                "@gap:platform": "ios",
